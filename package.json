{
  "name": "nats-client",
  "displayName": "NATS Client",
  "description": "Just like HTTP Client, but for NATS",
  "version": "0.0.1",
  "publisher": "mfahmialkautsar",
  "engines": {
    "vscode": "^1.106.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nats-client.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "bun run compile",
    "compile": "bun build src/extension.ts --outdir=out --target=node --external vscode --format=cjs",
    "watch": "bun build src/extension.ts --outdir=out --target=node --sourcemap=external --external vscode --format=cjs --watch",
    "pretest": "bun run compile && bun run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.106.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.46.3",
    "eslint": "^9.39.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
